<!-- Pizza Place -->
<!-- Displays all food types to select from -->
@using JoshMangoff_COMP2007_Assignment2.Models
@model IEnumerable<FoodType>

@{
    ViewBag.Title = "Index";
}

<h2>Browse Food Types</h2>
<p>
    Select from @Model.Count()
    Type of Food:

</p>
<ul>
    @foreach (FoodType item in Model)
    {
        <li> @Html.ActionLink(item.Name, "Browse", new { FoodType = item.Name }) </li>
    }

</ul>


